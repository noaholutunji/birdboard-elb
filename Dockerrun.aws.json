{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "app",
            "image": "noalistic/birdboard-app",
            "hostname": "app",
            "essential": false,
            "memory": 128
        },
        {
            "name": "web",
            "image": "noalistic/birdboard-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["app"],
            "memory": 128
        }
    ]
}
